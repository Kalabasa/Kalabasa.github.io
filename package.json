  "main": "index.js",
  "author": "Lean Rada",
  "devDependencies": {
    "autoprefixer-stylus": "^0.14.0",
    "browser-sync": "^2.24.5",
    "chokidar-cli": "^1.2.0",
    "concurrently": "^3.6.0",
    "esm": "^3.0.63",
    "glob": "^7.1.2",
